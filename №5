/* Напишите программу вычисляющую произведение двух целых чисел.
В программе гарантируется, что числа - целые.
В программе запрещается использовать операцию *(умножить) */

#include <iostream>

using namespace std;

int main()
{
    int a, b, s;
    s = 0;
    cin >> a >> b;
    if (a < 0 and b < 0)
    {
        b = -b;
        if (a < 0)
        {
            a = -a;
        }
    }
    if (b < 0)
    {
        b = -b;
        a = -a;
    }
    for (int i = 0; i < b; ++i)
    {
        s = s + a;
    }
    cout << s;
    return 0;
}
