/* Напишите программу проверяющую является ли число составным.
В программе гарантируется, что число - натуральное */

#include <iostream>

using namespace std;

int main ()
{
    int a;
    cin >> a;
    for (int i = 2; i != a; ++i)
    {
        if (a == 1)
        {
            cout << a << ": Unit";
            return 0;
        }
        if (a == 0)
        {
            cout << a << ": Zero";
            return 0;
        }
        if (a % i == 0)
        {
            cout << a << ": Composite";
            return 0;
        }
    }
    cout << a << ": Prime";
    return 0;
}
